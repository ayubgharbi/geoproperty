


<section class="form-auth text-center">
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <h1 class="h3 mb-3 font-weight-normal">Please Sign Up</h1>
  <%= render "devise/shared/error_messages", resource: resource %>

    <%= f.text_field :first_name, autofocus: true, autocomplete: "email ", class: "form-control", placeholder: "Email" %>
    <%= f.text_field :last_name, autofocus: true, autocomplete: "email ", class: "form-control", placeholder: "Email" %>
    <%= f.email_field :email, autofocus: true, autocomplete: "email ", class: "form-control", placeholder: "Email" %>
    <%= f.password_field :password, autocomplete: "new-password", class: "form-control", placeholder: "password" %>
    <%= f.password_field :password_confirmation, autocomplete: "new-password", class: "form-control", placeholder: "password_confirmation" %>

  <button class="btn btn-lg btn-primary btn-block" type="submit">Sign Up</button>
  <p class="mt-5 mb-3 text-muted">Â© 2017-2019</p>
<% end %>

<%= render "devise/shared/links" %>
</section>